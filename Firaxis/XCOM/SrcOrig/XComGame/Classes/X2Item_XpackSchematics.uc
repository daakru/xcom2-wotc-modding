class X2Item_XpackSchematics extends X2Item;

static function array<X2DataTemplate> CreateTemplates()
{
	local array<X2DataTemplate> Schematics;
	
	// Resistance Hero Weapon Schematics
	Schematics.AddItem(CreateTemplate_VektorRifle_Magnetic_Schematic());
	Schematics.AddItem(CreateTemplate_VektorRifle_Beam_Schematic());

	Schematics.AddItem(CreateTemplate_Bullpup_Magnetic_Schematic());
	Schematics.AddItem(CreateTemplate_Bullpup_Beam_Schematic());

	Schematics.AddItem(CreateTemplate_WristBlade_Magnetic_Schematic());
	Schematics.AddItem(CreateTemplate_WristBlade_Beam_Schematic());

	Schematics.AddItem(CreateTemplate_ShardGauntlet_Magnetic_Schematic());
	Schematics.AddItem(CreateTemplate_ShardGauntlet_Beam_Schematic());

	Schematics.AddItem(CreateTemplate_Sidearm_Magnetic_Schematic());
	Schematics.AddItem(CreateTemplate_Sidearm_Beam_Schematic());

	// Chosen Weapon Schematics
	Schematics.AddItem(CreateTemplate_ChosenShotgun_Schematic());
	Schematics.AddItem(CreateTemplate_ChosenSword_Schematic());
	Schematics.AddItem(CreateTemplate_ChosenSniperRifle_Schematic());
	Schematics.AddItem(CreateTemplate_ChosenSniperPistol_Schematic());
	Schematics.AddItem(CreateTemplate_ChosenRifle_Schematic());

	return Schematics;
}

